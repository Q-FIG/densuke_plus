{
  "manifest_version": 3,
  "name": "伝助+",
  "description": "伝助入力を便利にする",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://densuke.biz/event*",
        "https://densuke.biz/edit2?*"
      ],
      "js": ["content-scripts/edit.js"],
      "css": ["content-scripts/custom.css"]
    },
    {
      "matches": [
        "https://densuke.biz/list*"
      ],
      "js": ["content-scripts/list.js"],
      "css": ["content-scripts/custom.css"]
    }
  ]
}